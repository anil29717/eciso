{
  "version": 2,
  "builds": [
    {
      "src": "app_serverless.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app_serverless.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "functions": {
    "app_serverless.py": {
      "maxDuration": 10
    }
  }
}